@model List<CarDealership.Models.Vehicle>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
    <h2>Admin</h2>
    @Html.ActionLink("Add a new vehicle", "AddVehicle")
    <br />
    <div>
        @Html.Partial("_NewSearchBox")
    </div>
    <h2>Search Results</h2>
    <div id="errorMessages"></div>
    <div id="contentTiles">
        @foreach (var vehicle in Model)
        {
           Html.RenderPartial("_AdminSearchResult", vehicle);
        }       
    </div>    
</div>

